<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <defaults public="false" />

        <service id="maker.maker.graphql_type" class="Liinkiing\GraphQLMakerBundle\Maker\MakeGraphQLType">
            <argument>%kernel.project_dir%</argument>
            <tag name="maker.command" />
        </service>
        <service id="maker.maker.graphql_connection" class="Liinkiing\GraphQLMakerBundle\Maker\MakeGraphQLConnection">
            <argument>%kernel.project_dir%</argument>
            <tag name="maker.command" />
        </service>
        <service id="maker.maker.graphql_query" class="Liinkiing\GraphQLMakerBundle\Maker\MakeGraphQLQuery">
            <argument>%kernel.project_dir%</argument>
            <tag name="maker.command" />
        </service>
        <service id="maker.maker.graphql_mutation" class="Liinkiing\GraphQLMakerBundle\Maker\MakeGraphQLMutation">
            <argument>%kernel.project_dir%</argument>
            <tag name="maker.command" />
        </service>
    </services>
</container>
